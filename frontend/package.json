        "@types/react-dom": "^18.2.18",
        "@types/react-helmet": "^6.1.11",
        "@umijs/fabric": "^2.14.1",
        "@umijs/lint": "^4.1.1",
        "@umijs/max": "^4.1.1",
        "cross-env": "^7.0.3",
        "eslint": "^8.56.0",
        "express": "^4.18.2",
        "gh-pages": "^3.2.3",
        "husky": "^7.0.4",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "lint-staged": "^10.5.4",
        "mockjs": "^1.1.0",
        "prettier": "^2.8.8",
        "react-dev-inspector": "^1.9.0",
        "swagger-ui-dist": "^4.19.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3",
