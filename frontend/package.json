        "@uiw/react-markdown-preview": "^5.0.7",
        "@umijs/route-utils": "^2.2.2",
        "@xterm/addon-fit": "^0.9.0",
        "ag-grid-community": "^31.2.0",
        "ag-grid-enterprise": "^31.1.1",
        "ag-grid-react": "^31.1.1",
        "allotment": "^1.20.0",
        "antd": "^5.21.2",
        "antd-style": "^3.6.1",
        "browser-fs-access": "^0.35.0",
        "classnames": "^2.5.1",
        "hox": "^2.1.1",
